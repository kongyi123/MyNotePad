<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="titleTextColor">@color/colorBlack</item>
        <item name="android:windowLightStatusBar">@color/colorBlack</item>
        <item name="fontFamily">@font/whj000f0cb5</item>
        <item name="android:colorEdgeEffect">#FFAEC9</item>
        <item name="android:actionOverflowButtonStyle">@style/AppTheme.OverFlow</item>
        <item name="android:actionMenuTextColor">@color/colorBlack</item>
    </style>

    <style name="AppTheme.OverFlow" parent = "Widget.AppCompat.ActionButton.Overflow">
        <item name="android:tint">@color/colorBlack</item>
    </style>



</resources>